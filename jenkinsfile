pipeline {
    agent any
    stages {
        stage("st-1") {
            steps {
                echo "hello 3"
            }
        }
	stage("st-2") {
	    steps {
		sh 'cat /var/lib/jenkins/workspace/java-maven/jenkinsfile'

		}
    	}
	stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mava -cp target/my-app-1.0-SNAPSHOT.jar com.example.App'
            }
        }
	}
}
